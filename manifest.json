{
  "manifest_version": 2,
  "name": "Advanced Input Prefiller",
  "version": "1.4",
  "description": "Prefills multiple input fields on websites with advanced features",
  "permissions": [
    "activeTab",
    "storage",
    "downloads"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
